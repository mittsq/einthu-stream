<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
    <RootNamespace>EinthuStream</RootNamespace>
    <LangVersion>8</LangVersion>
    <PackageVersion>1.2.2</PackageVersion>
    <Title>EinthuStream</Title>
    <Authors>mittsquared</Authors>
    <Description>A client for Einthusan with casting and downloads.</Description>
    <RepositoryUrl>https://github.com/mittsquared/einthu-stream/</RepositoryUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    <PackageReference Include="RestSharp" Version="106.10.1" />
    <PackageReference Include="AngleSharp" Version="0.14.0" />
    <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="3.1.3" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="3.1.3" />
  </ItemGroup>
</Project>